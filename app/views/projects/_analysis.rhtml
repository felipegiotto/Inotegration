	<h3 class="<%= analysis.situation_verbose %>"><%= analysis.created_at %></h3>
	<p>Dados do commit: <pre><%=h analysis.dados_do_commit %></pre></p>
	<pre><%=h analysis.texto %></pre>
	<ul>
	<% analysis.resultados.each do |resultado| %>
	  <li class="<%= resultado.situation_verbose %>">
	  	<b><%=h resultado.nome %></b> (<%= resultado.tempo %>s)
	  	<pre><%=h resultado.texto %></pre>
	  </li>
	<% end %>
	</ul>
